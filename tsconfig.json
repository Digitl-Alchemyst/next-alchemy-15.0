{
  "compilerOptions": {
    "target": "ES2017",
    "lib": ["dom", "dom.iterable", "esnext"],
    "types": ["jest", "node"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    // "strictNullChecks": true,
    "forceConsistentCasingInFileNames": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    // "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./src/*"],
      "@/components/*": ["./src/components/*"],
      "@/lib/*": ["./src/lib/*"],
      "@/hooks/*": ["./src/hooks/*"],
      "@/utils/*": ["./src/util/*"],
      "@/types/*": ["./src/models/types/*"],
      "@/app/*": ["./src/app/*"],
      "@/s/*": ["./src/server/*"],
      "#/*": ["./*"]
    },
    "rootDir": "./",
    "baseUrl": ".",
    "outDir": "./out"
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts",
    "src/lib/jest/jest.config.ts",
    "src/lib/jest/jest.setup.ts",
    "build-utilities/generateRobots.mjs"
  ],
  "exclude": ["node_modules"]
}
